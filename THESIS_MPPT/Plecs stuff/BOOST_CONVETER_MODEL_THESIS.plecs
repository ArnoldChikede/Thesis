"\n"
"static double Ns;\n"
"static double Np;\n"
"\n"
"static double Tn;\n"
"static double Gn;\n"
"static double V_OC_n;\n"
"static double I_SC_n;\n"
"\n"
"static double Rs;\n"
"static double Rp;\n"
"\n"
"static double a;\n"
"\n"
"static double KV;\n"
"static double KI;\n"
"\n"
"static double k;\n"
"static double q;\n"
"\n"
"static double I_PV_n;\n"
"\n"
"#define I InputSignal(0,0)\n"
"#define V InputSignal(0,1)\n"
"#define G InputSignal(0,2)\n"
"#define T InputSignal(0,3)"
            Show          off
          }
          Parameter {
            Variable      "StartFcn"
            Value         "Ns = ParamRealData(0,0);\n"
"Np = ParamRealData(1,0);\n"
"\n"
"Tn = ParamRealData(2,0) + 273.15;\n"
"Gn = ParamRealData(3,0);\n"
"V_OC_n = ParamRealData(4,0);\n"
"I_SC_n = ParamRealData(5,0);\n"
"\n"
"Rs = ParamRealData(6,0);\n"
"Rp = ParamRealData(7,0);\n"
"\n"
"a = ParamRealData(8,0);\n"
"\n"
"KV = ParamRealData(9,0);\n"
"KI = ParamRealData(10,0);\n"
"\n"
"k = ParamRealData(11,0);\n"
"\n"
"q = ParamRealData(12,0);\n"
"\n"
"I_PV_n = I_SC_n;"
